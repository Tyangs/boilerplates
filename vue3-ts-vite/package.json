{
  "name": "vue-ts-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint src --ext ts,vue --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,vue --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write src --ext ts,tsx,vue,js,jsx,json,css",
    "preview": "vite preview",
    "cz": "npx git-cz"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "commitizen": "^4.3.0",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^8.41.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.14.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.4.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
